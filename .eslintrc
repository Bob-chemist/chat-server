{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true
      }
    ],
    "quotes": ["error", "single"]
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
